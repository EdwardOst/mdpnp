       <to uri="log:basicIce5"/>
      <to uri="rti:/29/ice::SampleArray(com.rti.dds.type.ice.SampleArray)/default"/>
    </route>

    <route id="basicIce6">
      <from uri="rti:/28/ice::Numeric(com.rti.dds.type.ice.Numeric)/default"/>
