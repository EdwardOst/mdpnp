    <route id="basicIce9">
       <from uri="rti:/28/ice::InfusionObjective(com.rti.dds.type.ice.InfusionObjective)/default"/>
       <to uri="log:basicIce9"/>
       <to uri="rti:/29/ice::InfusionObjective(com.rti.dds.type.ice.InfusionObjective)/default"/>
     </route>

     <route id="basicIce10">
       <from uri="rti:/28/ice::Text(com.rti.dds.type.ice.Text)/default"/>
       <to uri="log:basicIce10"/>
       <to uri="rti:/29/ice::Text(com.rti.dds.type.ice.Text)/default"/>
